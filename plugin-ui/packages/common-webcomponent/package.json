{
  "name": "@kchobantonov/common-jsonforms-webcomponent",
  "private": true,
  "version": "0.0.1-SNAPSHOT",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "wc-use-contenthash": "node ./patch-resolveWcEntry.js",
