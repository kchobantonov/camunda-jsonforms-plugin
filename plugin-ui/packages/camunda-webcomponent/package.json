{
  "name": "@chobantonov/camunda-jsonforms-webcomponent",
  "version": "0.0.1-SNAPSHOT",
  "scripts": {
    "dev": "vite",
    "prebuild": "rimraf dist",
    "build": "run-p type-check \"build-only {@}\" --",
