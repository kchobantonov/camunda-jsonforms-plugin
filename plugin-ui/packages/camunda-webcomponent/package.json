    "vite": "^5.2.8",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
