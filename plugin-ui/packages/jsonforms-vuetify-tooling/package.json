{
  "name": "jsonforms-vuetify-tooling",
  "displayName": "JSONForms Vuetify Tooling",
  "description": "VSCode extension for JSON Forms tooling using Vue2 Vuetify renderer set",
  "version": "0.0.1-SNAPSHOT",
  "repository": "https://github.com/kchobantonov/camunda-jsonforms-plugin",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "files": [
    "dist"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jsonforms-vuetify-tooling.showPreview",
        "title": "JSONForms Vuetify: Show Preview"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .json",
          "command": "jsonforms-vuetify-tooling.showPreview",
          "group": "jsonforms"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "build": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vscode/test-electron": "^2.2.3",
    "copy-webpack-plugin": "^6.4.0",
    "eslint": "^6.7.2",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^8.4.0",
    "typescript": "^4.7.3",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
    "@kchobantonov/common-jsonforms-webcomponent": "^0.0.1-SNAPSHOT",
    "chokidar": "^3.5.3",
    "util": "^0.12.5"
  }
}
