{
    "name": "@kchobantonov/camunda-jsonforms-plugin",
    "private": true,
    "scripts": {
        "init": "lerna bootstrap --hoist",
        "clean": "lerna run clean",
        "lint": "lerna run lint",
        "lint:fix": "lerna run lint:fix",
        "test": "lerna run test",
        "build": "lerna run build",
        "snapshot-version": "lerna version --no-git-tag-version --no-push --yes",
        "common:build": "lerna run --scope=@kchobantonov/common build",
        "common:test": "lerna run --scope=@kchobantonov/common test",
        "camunda:build": "lerna run --scope=@kchobantonov/camunda build",
        "camunda:test": "lerna run --scope=@kchobantonov/camunda test",
        "camunda-webcomponent:build": "lerna run --scope=@kchobantonov/camunda-webcomponent build",
        "camunda-webcomponent:test": "lerna run --scope=@kchobantonov/camunda-webcomponent test"
    },
    "devDependencies": {
        "lerna": "^4.0.0"
    }
}
