{
    "name": "@kchobantonov/camunda-jsonforms-plugin",
    "private": true,
    "scripts": {
        "init": "lerna bootstrap --hoist",
        "build": "lerna run build",
        "common:build": "lerna run --scope \"**/*common\" build",
        "common:watch": "cd common && npm run watch",
        "common:test": "lerna run --scope \"**/*common\" test",
        "camunda:build": "lerna run --scope \"**/*camunda\" build",
        "camunda:watch": "cd camunda && npm run watch",
        "camunda:test": "lerna run --scope \"**/*camunda\" test",
        "clean": "lerna run clean",
        "camunda-webcomponent:build": "cd camunda-webcomponent && npm run build"
    },
    "devDependencies": {
        "lerna": "^4.0.0"
    }
}
